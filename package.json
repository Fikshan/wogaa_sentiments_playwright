{
  "name": "wogaa-playwright-tests",
  "version": "1.0.0",
  "type": "module",
  "description": "Playwright automation framework for testing WOGAA Sentiments widget",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:qa": "cross-env ENV=qa npm run test:allure",
    "test:staging": "cross-env ENV=staging npm run test:allure",
    "test:production": "cross-env ENV=prod npm run test:allure",
    "report": "allure generate allure-results --clean -o allure-report && allure open",
    "clean-results": "rimraf allure-results",
    "test:negative": "npx playwright test negativeRatings.spec.js",
    "test:positive": "npx playwright test positiveRatings.spec.js",
    "test:helpUsImprove":"npx playwright test helpUsImprove.spec.js",
    "test:allure": "npm run clean-results && npm run test && npm run report",
    "lint": "eslint . --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fikshan/wogaa.git"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "qa",
    "wogaa",
    "javascript",
    "end-to-end",
    "e2e"
  ],
  "author": "Suraj Suri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fikshan/wogaa/issues"
  },
  "homepage": "https://github.com/Fikshan/wogaa#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.53.1",
    "allure-commandline": "^2.34.1",
    "allure-js-commons": "^3.3.0",
    "allure-playwright": "^3.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "dotenv": "^16.6.1"
  }
}
